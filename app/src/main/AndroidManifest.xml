<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bimurtoit.com.calculatecgpa">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_cgpa"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bimurtoit.com.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.bimurtoit.com.activity.CalculateCGPAActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity android:name="com.bimurtoit.com.activity.ResultBookActivity"
            android:windowSoftInputMode="adjustPan">

        </activity>
        <activity android:name="com.bimurtoit.com.activity.CourseListActivity">

        </activity>
    </application>
</manifest>
